function updateTemplateEleParam(){
    var parameters = $("#templateElementParams").find(".elemParameter");
    for(var i=0;i<parameters.length;i++){
        var name = $(parameters[i]).attr("name");
        var value = $(parameters[i]).is(':checkbox') ? $(parameters[i]).get(0).checked : $(parameters[i]).val() ;
        $(editingTemplateElement).attr("amphi-param-"+name,value);
    }
    var custParameters = $("#templateElementParams").find(".elemCustParameter");
    var custParamHtml = "";
    for(var i=0;i<custParameters.length;i++){
        var name = $(custParameters[i]).attr("name");
        var value = $(custParameters[i]).is(':checkbox') ? $(custParameters[i]).get(0).checked : $(custParameters[i]).val() ;
        var fname = $(custParameters[i]).attr("fname");
        custParamHtml += "<div>"+(fname!=null ? fname : name)+" : "+value+"</div>";
        $(editingTemplateElement).attr("amphi-cust-param-"+name,value);
    }
    $(editingTemplateElement).find(".customParam").html(custParamHtml);
    var content = $("#templateElementParams").find("[name=textcontent]");
    if(content.length>0){
        collectCKeditorValues();
        $(editingTemplateElement).find(".textContent").val($("#templateElementParams").find(".elemContent").val());
    }
    serializeTemplate()
}

function fillTemplateElementParam(){
    var eleAttributes = editingTemplateElement.get(0).attributes;
    for(var i=0;i<eleAttributes.length;i++){
        if(eleAttributes[i].name.indexOf("amphi-param-")==0){
            $("#templateElementParams").find("[name="+(eleAttributes[i].name.substring(12))+"]").get(0).value=eleAttributes[i].value;
            if($("#templateElementParams").find("[name="+(eleAttributes[i].name.substring(12))+"]").is(':checkbox')){
                $("#templateElementParams").find("[name="+(eleAttributes[i].name.substring(12))+"]").get(0).checked = eleAttributes[i].value=='true';
            }
        }
        else if (eleAttributes[i].name.indexOf("amphi-cust-param-")==0){
            $("#templateElementParams").find("[name=" + (eleAttributes[i].name.substring(17)) + "]").get(0).value = eleAttributes[i].value;
            if ($("#templateElementParams").find("[name=" + (eleAttributes[i].name.substring(17)) + "]").is(':checkbox')) {
                $("#templateElementParams").find("[name=" + (eleAttributes[i].name.substring(17)) + "]").get(0).checked = eleAttributes[i].value == 'true';
            }
        }
    }
    if($(editingTemplateElement).find(".textContent").size()>0){
        $("#templateElementParams").find(".elemContent").val($(editingTemplateElement).find(".textContent").val());
        richTextEditor('textcontent','reportTemplate_'+$("#templateId").val(),'any');
    }
}

function serializeTemplate(){
    var template={courseId:$('#courseId').val(),name:$("#reportName").val(),templateId:$("#templateId").val(),elements:[],roles:[]};
    var childrens = $('#reportTeplatePane').children();
    for(var i=0;i<childrens.length;i++){
        var id=$(childrens[i]).attr('amphi-element-id');
        var type=$(childrens[i]).attr('amphi-element-type');
        var params={};
        var customParams={};
        var eleAttributes = childrens[i].attributes;
        for (var j = 0; j < eleAttributes.length; j++) {
            if (eleAttributes[j].name.indexOf("amphi-param-") == 0) {
                params[eleAttributes[j].name.substring(12)]=eleAttributes[j].value;
            }
            else if(eleAttributes[j].name.indexOf("amphi-cust-param-") == 0){
                customParams[eleAttributes[j].name.substring(17)]=eleAttributes[j].value;
            }
        }
        if($(childrens[i]).find(".textContent").length>0){
            params['content']=$(childrens[i]).find(".textContent").val();
        }
        template.elements.push({id:id,type:type,params:params,customParams:customParams});
    }
    var x = document.getElementById("roles");
    for (var i = 0; i < x.options.length; i++) {
        if (x.options[i].selected == true) {
            template.roles.push(x.options[i].value);
        }
    }
    return template
}

