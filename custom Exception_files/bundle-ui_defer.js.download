var language = {
    stPos:[],
    enPos:[],
    name:[],
    addCheckPoint: function(){
       showWait();
        var st = parseInt(document.getElementById('checkStartPosition').value);
        var en = parseInt(document.getElementById('checkEndPosition').value);
        var n = document.getElementById('name').value;
        var f = 1;
        if(st >= en)
            f = 0;
        
        var i=0;
        if(n.trim().length == 0){
            f = 0;
        }
        while(i < language.stPos.length){
            if(st == language.stPos[i] && en == language.enPos[i])
                f = 0;
            i++;
        }
        if(f == 1){
            language.stPos.push(st);
            language.enPos.push(en);
            language.name.push(n);
            language.checkPointList();
        }
    },
    checkPointList: function(){
           var checkpoint = ''
        document.getElementById('checkPointList').innerHTML = '';
        var i=0;
        var s;
        var e;
        var n;
        for(i=0; i <language.stPos.length; i++){
            s = '<input class="form-control" value="'+language.stPos[i]+'" name="st" readonly="readonly" >';
            e = '<input class="form-control" value="'+language.enPos[i]+'" name="en" readonly="readonly" >';
            n = '<input class="form-control" value="'+language.name[i]+'" name="name">';
            document.getElementById('checkPointList').innerHTML += '<br/>';
            document.getElementById('checkPointList').innerHTML += '<strong>Problem '+(i+1)+'</strong>';
            document.getElementById('checkPointList').innerHTML += '<br/>';
            document.getElementById('checkPointList').innerHTML += '<br/>';
            document.getElementById('checkPointList').innerHTML += "<table class='table table-striped table-bordered'><tr><td>Name</td><td>"+n+"</td></tr><tr><td>Start Line</td><td>"+s+"</td></tr><tr><td>End Line</td><td>"+e+"</td></tr></table><a class='btn btn-danger' href='javascript:language.deleteCheckPoint("+i+")'>Delete</a>"
            document.getElementById('checkPointList').innerHTML += '<br/>';
            
        }
        if(language.stPos.length > 0){
            document.getElementById('checkPointSave').style.display = 'block';
        }
        hideWait();
    },
    deleteCheckPoint: function(i){
        if(confirm('Are you sure?')){
            language.stPos.splice((i-1), 1);
            language.checkPointList();
        }
    },
    subSetDialogue: function(){
        var st = document.getElementById('checkStartPosition').value;
        var en = document.getElementById('checkEndPosition').value;
        var size = document.getElementById('checkEndPosition').options.length
        var i = 0;
        for(i = 0; i < size; i++){
            if(i >= (st-1) && i <  en)
                document.getElementById('diag'+i).style.display = '';
            else
                document.getElementById('diag'+i).style.display = 'none';
        }

    },
