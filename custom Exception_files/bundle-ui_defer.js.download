/* XML Parsing CallBack For DOJO requests */

function parseResponseAndUpdateInitVal(response, outputElement, keyword,allUser) {
    parseResponseForDojoXML(response, outputElement, keyword,allUser)
    if($(outputElement).attr('initloaded') == 'false'){ 
        $(outputElement).val($(outputElement).attr('initval')).trigger('change');
        $(outputElement).attr('initloaded','true')
    }else{ 
        if(response.getElementsByTagName(keyword).length > 0) {
            var elements = response.getElementsByTagName(keyword);
            var messageId = elements[0].getElementsByTagName("id")[0].childNodes[0].nodeValue;
            $(outputElement).val(messageId).trigger('change');
        }
        else{
            $(outputElement).val('').trigger('change');
        }
    }
}
function parseResponseForDojoXML(response, outputElement, keyword,allUser) {
  if(outputElement !== null) {
      for(i=outputElement.length-1;i>=0;i--) {
        outputElement.remove(i);
      }
      if(response.getElementsByTagName(keyword).length > 0) {
          if(allUser=="true"){
                newOption=document.createElement('option');
                newOption.text = 'All';
                newOption.value = '';
                if (browser === "Microsoft Internet Explorer")
                    outputElement.add(newOption);
                else
                    outputElement.appendChild(newOption);
          }
          var elements = response.getElementsByTagName(keyword)
        for(i=0;i<elements.length;i++) {
            messageId = elements[i].getElementsByTagName("id")[0].childNodes[0].nodeValue;
            messageValue = elements[i].getElementsByTagName("name")[0].childNodes[0].nodeValue;
            newOption=document.createElement('option');
            newOption.text = messageValue;
            newOption.value = messageId;
            if (browser==="Microsoft Internet Explorer")
              outputElement.add(newOption);
            else
              outputElement.appendChild(newOption);
        }
      }
  }
}

