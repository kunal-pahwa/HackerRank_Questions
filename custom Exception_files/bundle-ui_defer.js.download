    function play(obj){
        var iframe = document.getElementById('slideIframe');
        innerDoc = iframe.contentDocument || iframe.contentWindow.document;
        currentSlide = getCurrentPPTSlide();
        var plyState = -1;
        var song = $("#audioPlayer").find('audio').get(0);
        var srcTag = $(song).html("<source src='' type='audio/mpeg'>");
            if($(obj).attr('id') != 'frwdBtn'){
                if($("#mobileView").val() == 1){
                    if($(obj).hasClass("mbl-pause")){
                        plyState = 1;
                        $(obj).css('display','none');
                        $('#playBtn').css('display','');     
                        if(song != undefined)
                            song.pause();
                        c = 0
                    }else if($(obj).hasClass("mbl-play")){
                        $(obj).css('display','none');
                        $('#pauseBtn').css('display','');
    //                    alert("totalSlide "+totalSlide+" currentSlide "+currentSlide)
                        if(song!=undefined){
                            if(song.paused)
                                song.play();
                            else
                                run(presentSlide);
                        }
                    }
                }else{
                    var currentView = $("#lessonCompView").val();
                    var playContent = "",pauseContent="";
                    if(currentView=="online") {
                        playContent='<i class="fa fa-play"></i>';
                        pauseContent='<i class="fa fa-pause"></i>';
                    }
                    else {
                        playContent='Play <i class="fa fa-play"></i>';
                        pauseContent='Pause <i class="fa fa-pause"></i>';
                    }
                    if($(obj).children( ':first' ).hasClass("fa-pause")){
                        plyState = 1;
                        $(obj).html(playContent);                                                                          
                        if(pptObj != undefined)
                            pptObj.pause();
                    }else if($(obj).children( ':first' ).hasClass("fa-play")){
                        $(obj).html(pauseContent);
                        if(pptObj!=undefined){
                            if(pptObj.isPaused()){
                                pptObj.play();
                            }else{
                                pptObj.play(Number((presentSlide)-1));
                            }
                        }                    
                    }
                }
                if(plyState == 1){
                    try{
                        iframe.contentWindow.pause();
                    }catch(err){
                        iframe.contentWindow.Presentation.Pause();
                    }
                    c = 0;
                }else{
                    if(totalSlide == currentSlide){
                        try{
                            iframe.contentWindow.gotoSlide(1);
                        }catch(err){
                            iframe.contentWindow.Presentation.GotoSlide(1);
                        }
                        if($('#slideControl').val() == "false"){
                            if(pptObj != undefined)
                                pptObj.play(0); 
                        }
                        innerDoc.getElementById("current").value = 1;
                        presentSlide = 1;
                    }
                    try{
                        iframe.contentWindow.play();
                    }catch(err){
                        iframe.contentWindow.Presentation.Play();
                    }                                                
                    if(c == 1){            
                        next($('#compId').val(),'next',(Number(currentSlide)+1));
                        c = 0;
                    }else{
                        c++;
                    }
                }
            }else{
                next($('#compId').val(),'next',(Number(presentSlide)+2));
            }
        }

