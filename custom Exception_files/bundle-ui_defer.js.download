var digital_problems={};
var kmap_patern_var=0;
var canEditKMap=true;
var kmapreadyflag=0;
var existingKmap=[];
var kmapAccepted=false;
var contextMenutop=250;
var contextMenuleft=50;

function kmapready(){
    if(kmapreadyflag==0){
        kmapreadyflag=1;
        digital_problems={};
        jQuery('.kmapValues').click(function(e){
            if(canEditKMap){
                if(jQuery(this).hasClass('selected'))
                    jQuery(this).removeClass('selected');
                else
                    jQuery(this).addClass('selected');
            }
        });
        jQuery('.kmapValues').bind('contextmenu', function(e){
            e.preventDefault();
            var key=jQuery(this).attr('id')
            var grps=[]
            for(var dp in digital_problems){
                kmaps=digital_problems[dp].kmaps;
                for(var k in kmaps)
                {
                    var inp=kmaps[k]
                    for(var i in inp.groups){
                        for(var j in inp.groups[i]){
                            if(inp.groups[i][j]==key){
                                grps.push({
                                    kmap:k,
                                    group:i,
                                    problem:digital_problems[dp].id
                                })
                                break;
                            }
                        }
                    }
                }
                if(grps.length>1){
                    var hts="<div class='flyOverMenu'>"
                    for(var i=0;i<grps.length;i++){
                        hts+="<div class='group' onmouseover='highlight({kmap:"+grps[i].kmap+",group:"+grps[i].group+",problem:"+grps[i].problem+"})' onmouseout='unhighlightGroup()' onmousedown='ungroup({kmap:"+grps[i].kmap+",group:"+grps[i].group+",problem:"+grps[i].problem+"})' id='key-"+grps[i]+"' >Group {kmap:"+grps[i].kmap+",group:"+grps[i].group+",problem:"+grps[i].problem+"}</div>"
                    }
                    hts+="</div>"
                    jQuery('#contextMenu').html(hts)
                    jQuery('#contextMenu').css({
                        top: e.pageY + 'px',
                        left: e.pageX + 'px'
                    }).show();
                }
                else if(grps.length==1){
                    ungroup(grps[0])
                }
            }
            return false;
        });
        jQuery('.kmapValues').mouseover(function(){
            var id = jQuery(this).attr('id');
            selectGroupWithId(id);
        });
        jQuery('.kmapValues').mouseout(function(){
            jQuery('.kmapValues').removeClass('highlightGroup')
        });
        jQuery(document).mousedown(function(){jQuery('#contextMenu').hide()});
        if(existingKmap!=undefined){
            writeExistingGroup(existingKmap);
        }
    }
}
function highlight(o){
    var dp=Number(o.problem);
    var km=Number(o.kmap);
    var g=Number(o.group);
    if(digital_problems[dp]==undefined){
        digital_problems[dp]={id:dp,kmaps:[]}
    }
