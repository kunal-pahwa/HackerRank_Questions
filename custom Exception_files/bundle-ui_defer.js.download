//                    //alert(data.checkPoint);
//                    window.location.href=contextPath+"/lessonComponent/redirectToSession/"+data.checkPoint;
//                }
            }else{
                alert("This page is disable");
            }
        }        
        else{            
            var url = contextPath+"/lessonComponent/fetchImageWithConstraint/"+data.id+"?compResourceId="+comResourceId+"&require=fullimage";
            lessonComponent.loadSlideResource(data.imgId,autoPlay);
            if(autoPlay == false){
                obj = []
            }
            lessonComponent.loadResource(data.imgId,autoPlay); 
