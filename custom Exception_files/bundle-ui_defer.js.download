function isNumber(evt,ele) {
        evt = (evt) ? evt : window.event;       
        var charCode = (evt.which) ? evt.which : evt.keyCode;
        if (charCode > 31 && (charCode < 48 || charCode > 57)) {
            if(charCode == 46){
                if($(ele).val().indexOf('.') == -1){
                    return true
                }
            }
            else if(charCode == 45){
                if($(ele).val().length == 0){
                    return true
                }
            }
            return false;
        }
        return true;
    }
    
