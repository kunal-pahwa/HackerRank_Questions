$(function() {
  update_clock();
  var wait_time = 0;
  var master_timer = 0
  time_interval = setInterval(function() {
    update_clock();
    master_timer++;
    if (master_timer - wait_time > 0) {
      $('#late-time').html('You are now ' + convert_seconds(master_timer - wait_time) + ' late.');
    }
    if (elapsed >= wait_time && showFinishButton == true) { // time up
        document.getElementById('optionType').value = 'Finish';
        document.getElementById('automatic').value = 'true';
        document.attemptForm.submit();
      //$('title').html('CD: late ' + convert_seconds_compact(master_timer - wait_time));
    } else { // not time up yet
      //if (wait_time > 0) {
        //$('title').html('CD: ' + convert_seconds_compact(wait_time - master_timer));
      //}
    }
  }, 1000);
  
  var q = $('#q').html();
  
  var match = /^([0-9]+)$|^((([0-9]+)h)?(([0-9]+)m)?(([0-9]+)s)?)$|^(([0-9]+)(a?p?m?))$/i.exec(q);

  
  if (match != null) {
    if (match[9] != null && match[9] != '' && parseInt(match[10]) > 0 && parseInt(match[10]) <= 1259) { // a time input
      var current = new Date();
      var now = current.getTime();
      var hr;
      var min;
      if (match[10].length == 1 || match[10].length == 2) { // only the hour given
        hr = parseInt(match[10]);
        min = 0;
      } else if (match[10].length == 3) {
        hr = parseInt(match[10].substr(0,1));
        min = parseInt(match[10].substr(-2));
      } else if (match[10].length == 4) {
        hr = parseInt(match[10].substr(0,2));
        min = parseInt(match[10].substr(-2));
      }
      
      if (min < 60) {
        if (match[11].substr(0,1) == "p") {
          if (hr != 12) {
            hr += 12;
          }
        } else if (match[11].substr(0,1) == "a" && hr == 12) {
          hr -= 12;
        }
        var target = new Date(current.getFullYear(),current.getMonth(),current.getDate(),hr,min,0,0).getTime();
        if ((target-now) < 0) {
          target += 86400000; // add one day
        }
        wait_time = Math.floor((target-now)/1000);
      } else {
        wait_time = -1;
      }
    } else if (match[1] != null && match[1] != '') { // just a number
      wait_time = parseInt(match[1]) * 60; // assume number is for minutes by default
    } else {
      if (match[4] != null && match[4] != '') { // hours
        wait_time += parseInt(match[4],10) * 3600;
      }
      if (match[6] != null && match[6] != '') { // minutes
        wait_time += parseInt(match[6],10) * 60;
      }
      if (match[8] != null && match[8] != '') { // seconds
        wait_time += parseInt(match[8],10);
      }
    }
  }
  
  if (wait_time > 0) {
    if (wait_time <= 172800) {
      var elapsed = 0;
      var time_interval = 0;
      var timer_interval = 0;
      $('#timeleft').html(convert_seconds(wait_time));
      $('#percentgone').html('0%');
      timer_interval = setInterval(function() {
        elapsed++;
        $('#cdbar').width(elapsed/wait_time * 758);
        $('#timeleft').html(convert_seconds(wait_time - elapsed));
        $('#percentgone').html(Math.floor(elapsed/wait_time*100) + '%');
        if (elapsed >= wait_time) { // time up

          clearInterval(timer_interval);
          $('#cdbar').css('background','#990000');
          $('#percentgone').html('');
          $('#timeleft').html((convert_seconds(wait_time) + ' passed'));
          $('#late').show();
        }
      }, 1000);
    } else {
      wait_time = -1;
      $('#timeleft').html('Too much Time set ...');
    }
  } else {
    if (q == '') {
      $('#timeleft').html('0 seconds elapsed');
      var stopwatch_interval = 0
      stopwatch_interval = setInterval(function() {
        $('#timeleft').html(convert_seconds(master_timer) + ' elapsed');
      }, 1000);
    } else {
      $('#timeleft').html('Invalid input');
    }
  }
});

function plural(num) {
  if (num == 1) return '';
  return 's';
}

function update_clock() {
  var hr = new Date().getHours();
  var min = new Date().getMinutes();
  var sec = new Date().getSeconds();

  //$('#now').html('<strong>Now:</strong> ' + hr + ':' + pad_zero(min) + ':' + pad_zero(sec));
}

function convert_seconds(t) {
  var h = Math.floor(t/3600);
  var m = Math.floor((t - h*3600)/60);
  var s = Math.floor(t - h*3600 - m*60);
  var time_left = '';
  if (h > 0) time_left += h + ' hour' + plural(h) + ' ';
  if (m > 0) time_left += m + ' minute' + plural(m) + ' ';
  if (s > 0 || (h==0 && m==0)) time_left += s + ' second' + plural(s);
  return time_left;
}

function convert_seconds_compact(t) {
  var h = Math.floor(t/3600);
  var m = Math.floor((t - h*3600)/60);
  var s = Math.floor(t - h*3600 - m*60);
  var time_left = '';
  if (h > 0) time_left += h + 'h';
  if (m > 0) time_left += m + 'm';
  if (s > 0 || (h==0 && m==0)) time_left += s + 's';
  return time_left;
}

function pad_zero(num) {
  if (num < 10) return '0' + num;
  return num;
}

