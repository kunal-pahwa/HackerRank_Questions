function reArrangeCourseCategory(pUrl){
    var roadMapIds = $('#roadMapIds').val();
    var queryString = "";
    for(var i=0;i<roadMapIds.length;i++){
        if(i !== 0){
            queryString += '&';
        }
        queryString += 'roadMapId='+roadMapIds[i];
    }
    $.ajax({
        url:pUrl + "?" + queryString,
        before:showWait(),
        success: function(data) {
            $('#rearrangeElements').html(data);
            $('#rearrangeDialog').modal('show');
            $('#rearrangeDialog').on('shown.bs.modal',function(){
                amphiSortable(".rearrangeElements",function($item){
                    var ids=[];
                    $item.parent().children().each(function(){
                          ids.push($(this).attr("id"));
                    });
                    var data=ids.join();
                    document.getElementById('clusterIds').value = data;
                });
            });
            hideWait();
        },
        error: function(data){
            hideWait();
        }
    });
}

function changeVisibilitySchedule(url) {
    $.ajax({
        url:url,
        success: function(data) {
            $('#teamschedulecontent').html(data);
            hideWait();
        }
    });
}
