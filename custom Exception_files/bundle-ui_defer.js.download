function computeMatch(content,pattern) {
    var dmp = new diff_match_patch();
    dmp.Diff_Timeout = 0;
    dmp.Match_Distance = 500
    dmp.Match_Threshold = 0.3
    var loc = 0
    var matchLoc = -1;
    while(loc < content.length) {
        matchLoc = dmp.match_main(content,pattern,loc)
        if(matchLoc != -1)
            break;
        loc = loc + (dmp.Match_Distance * dmp.Match_Threshold)
    }
    return matchLoc
}

function highlightMatches(content, matches) {
    for(i=0;i<matches.length;i++) {
        content = content.replace(matches[i],"<span style='color:red'>"+matches[i]+"</span>")
    }
    return content;
}
function slicePattern(content) {
       var len=20, curr = len, prev = 0;

       output = [];

       while(content[curr]) {
         if(content[curr++] == ' ') {
           output.push(content.substring(prev,curr));
           prev = curr;
           curr += len;
         }
       }
       output.push(content.substr(prev));
       return output;
}
function getPlainText(html) {
    return html.replace(/<[^<|>]+?>/gi,"");
}
function getTopicContent() {
    var editor = CKEDITOR.instances.content
