function sortableTable(tableId,enablePaginate,enableFilter,colType,unsortColClass,callBackFun){
    var table = $('#'+tableId).DataTable({
        "bPaginate": enablePaginate,
        "bFilter":enableFilter,
        "columnDefs": colType
    });
    if(callBackFun != undefined && callBackFun != null){
        $('#'+tableId)
            .on( 'order.dt',  function () { callBackFun(unsortColClass);})
            .DataTable();
    }
    var tt = new $.fn.dataTable.TableTools( table );
    $( tt.fnContainer() ).insertBefore('div.dataTables_wrapper');        
}

function disableUnSortableCol(className) {
    $("."+className).removeClass("sorting_asc");
    $("."+className).removeClass("sorting_desc");
    $("."+className).removeClass("sorting");
}

function sortableTableNew(tableId,enablePaginate,enableFilter,colType,fixedColIdx,enableLengthChange){
    if(enableLengthChange == undefined)
        enableLengthChange = false
    var table = $('#'+tableId).DataTable({
        "bPaginate": enablePaginate,
        "bFilter":enableFilter,
        "bLengthChange": enableLengthChange,
        "columnDefs": colType,        
    });
    var tt = new $.fn.dataTable.TableTools( table );
    $( tt.fnContainer() ).insertBefore('div.dataTables_wrapper');        
//    table.fnSortListener( document.getElementById('sorter'), 1 );
    if(fixedColIdx != undefined && fixedColIdx > -1){
        $('#'+tableId).on( 'order.dt', function(){
            var a = table
                .columns(fixedColIdx)
                .data()
                .each(function(d){
                    sortSNoAsc(tableId,d,fixedColIdx);
                });
        } );
    }
}

function sortSNoAsc(tableId,d,fixedColIdx){
    var idx = 0,i = 0;
    d.sort(function(a,b){
        return a-b;
    });
    $("#"+tableId+" tbody tr").each(function(){
        idx = 0;
        $(this).find("td").each(function(){
            if(idx == fixedColIdx){
                $(this).html(d[i]);
                i++;
            }
            idx++;
        });
    });
}


