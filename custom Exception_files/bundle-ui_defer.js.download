    mediaElement:"",
    intervalVar:"",
    initVideo: function(){
        if(document.getElementById('youtubeElement')){
            if(!document.getElementById('videoElementCreate')){
                lessonComponent.intervalVar=self.setInterval(lessonComponent.checkYouTubeValidity,1000);
            }
        }
        else{
            if(document.getElementById('lessonCompView').value == "online") {
                lessonComponent.intervalVar=self.setInterval(lessonComponent.checkOnlineVideoValidity,1000);
            }
            else if(!document.getElementById('videoElementCreate')) {
                lessonComponent.intervalVar=self.setInterval(lessonComponent.checkVideoValidity,1000);
            }
        }
    },
    checkVideoValidity: function(){
        mediaElement = lessonComponent.mediaElement;
        if(Math.floor(mediaElement.duration) > 0){
            if(parseInt(document.getElementById('checkPointId').innerHTML) == -1){
                clearInterval(lessonComponent.intervalVar);
            }
            if((Math.floor(mediaElement.currentTime)) >= (Math.floor(mediaElement.duration)-1)){
                mediaElement.pause();
                if(confirm("You must attend a session before proceeding")){
                    clearInterval(lessonComponent.intervalVar);
                    window.location.href=contextPath+"/lessonComponent/redirectToSession/"+parseInt(document.getElementById('checkPointId').innerHTML);
                }
                else{
                    clearInterval(lessonComponent.intervalVar);
                    document.getElementById('takeTest').style.display = 'block';
                }
            }

        }
        //alert(mediaElement);
    },
    checkOnlineVideoValidity: function(){
        mediaElement = lessonComponent.mediaElement;
        if(Math.floor(mediaElement.duration) > 0){
            if(parseInt(document.getElementById('checkPointId').innerHTML) == -1) {
                clearInterval(lessonComponent.intervalVar);
            }
            mediaElement.addEventListener('ended', function(){
                document.getElementById('lesson-component-video-tooltip').style.display = "block";
                showLessonBlurEffect();
            });
            document.getElementById('lesson-component-video-tooltip-ok').onclick = function() {
                clearInterval(lessonComponent.intervalVar);
                document.getElementById('lesson-component-video-tooltip').style.display = "none";
                window.location.href=contextPath+"/lessonComponent/redirectToSession/"+parseInt(document.getElementById('checkPointId').innerHTML);
            };
            document.getElementById('lesson-component-video-tooltip-cancel').onclick = function() {
                clearInterval(lessonComponent.intervalVar);
                document.getElementById('lesson-component-video-tooltip').style.display = "none";
                removeLessonBlurEffect();
            };
        }
    },
    takeTest: function(){
        window.location.href=contextPath+"/lessonComponent/redirectToSession/"+parseInt(document.getElementById('checkPointId').innerHTML);
    },
    changeCurrentPosition: function(val){
      var tempPtr = parseInt(document.getElementById('checkPosition').value);
      
      mediaElement = lessonComponent.mediaElement;
      if(val == 'back')  
          tempPtr--;
      else if(val == 'next')
          tempPtr++;
      if(tempPtr < 5)
          tempPtr = 5;
      if(tempPtr >= Math.floor(mediaElement.duration))
          tempPtr = Math.floor(mediaElement.duration);
      document.getElementById('checkPosition').value = tempPtr;
      document.getElementById('checkPositionTime').innerHTML = ""+(parseInt(tempPtr/3600)%24)+":"+(parseInt(tempPtr/60)%60)+":"+(tempPtr%60);
      
    },
