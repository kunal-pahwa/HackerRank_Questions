function ungroup(o){
    if(canEditKMap){
        console.log(digital_problems)
        var dp=Number(o.problem);
        var km=Number(o.kmap);
        var g=Number(o.group);
        for(var i in digital_problems[dp].kmaps[km].groups[g]){
            if(i!="groupPattern"){
                var flag=0;
                for(var j in kmaps[km].groups){
                    if(kmaps[km].groups[j].indexOf(kmaps[km].groups[g][i])!=-1&&g!=j){
