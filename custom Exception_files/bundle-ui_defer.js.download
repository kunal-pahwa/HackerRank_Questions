    function getCurrentPPTSlide(){
        var active = innerDoc.getElementsByClassName("SC");
        var i = 0;
        $(active).each(function(){
            i++;
            if($(this).css("display")=="block"){
                currentSlide = i;
            }
        });
        return currentSlide;
    }
    
    function next(comId,seek,slideNumber){
        var currentSlide 
        var status = false;
        var iframe = loadIframeDoc("slideIframe");
        innerDoc = iframe.contentDocument || iframe.contentWindow.document;
        currentSlide = getCurrentPPTSlide()
        if(seek == 'next'){            
            if(totalSlide == currentSlide){
                try{
                    iframe.contentWindow.gotoSlide(1);
                }catch(err){
                    iframe.contentWindow.Presentation.GotoSlide(1);
                }
                presentSlide = 1
            }else{                
                if($('#slideControl').val() == "true"){
                    presentSlide = Number(currentSlide)+1;
                }
            }
        }else if(seek == 'prev'){
            if(currentSlide == 1){
                currentSlide = Number(1);
            }else{
                currentSlide = Number(currentSlide)-1;                
            }
            presentSlide = Number(currentSlide);                
        }
        if(slideNumber!= undefined && slideNumber!= 'undefined'){
            isCheckPointPresent(slideNumber,comId,seek);
        }else{
            isCheckPointPresent(presentSlide,comId,seek);
        }
    }
    
