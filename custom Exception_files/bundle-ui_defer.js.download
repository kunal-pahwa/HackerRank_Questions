		var divs_bts;
		var index = 0;
		var loading =  [];
		loading.push(div_btn_1,div_btn_2, div_btn_3);


		$.each(loading, function(i, index){

			divs_bts = $(center_loading).append(index);

		});
		
		if(value == 'small'){
			var small = $(divs_bts).addClass('small');
