    validateFetchImage: function(seekVal, comResourceId, seekPtr, autoPlay){        
        var url;
        var btnState = $("#playPauseBtn").html();
        var stat = $("#playPauseBtn").attr("btnState");
        if(autoPlay == false){
            showWait();
            nextObjReady = false;
            urlPresent = 0;
        }
        var currentPtr
        if(document.getElementById('slideNumber')!=null)
            currentPtr = document.getElementById('slideNumber').innerHTML.trim();
        $.ajax({
            type: "POST",
            url: contextPath+"/lessonComponent/validateFetchImage",
            data: {
                'seekVal': seekVal, 
                'currentPtr': currentPtr,
                'id':comResourceId
            },
            dataType: "json",
            success: function(data) {
                if(data.enable == 'true' || data.enable == 'null'){
                    if(autoPlay == true){
                        url = lessonComponent.validateFetchImageLoad(data, comResourceId, currentPtr, autoPlay);
                        nextObjReady = true
                        document.getElementById('nextHiddenImage').innerHTML = "<img src='"+url+"' width='700' style='border:1px solid #CCC'/>";                    
                    }else{
                        if(obj!= undefined && obj[0] != undefined && seekVal != 'goto')
                            obj[0].play();
                        lessonComponent.validateFetchImageLoad(data, comResourceId, currentPtr, autoPlay);
                    }                
                    $("#playPauseBtn").html(btnState);
                    $("#playPauseBtn").attr("btnState",stat);
                }else if(data.enable == 'false'){
                     alert("This page is disable");
                     hideWait();
                }
            },
            error: function (request, status, error) {
                hideWait();
                alert(error);
            }
        });
    },    
