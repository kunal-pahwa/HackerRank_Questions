function group(k,dp){
    if(canEditKMap){
        if(digital_problems[dp]==undefined){
            digital_problems[dp]={id:dp,kmaps:[]}
        }
        if(digital_problems[dp].kmaps[k]==undefined){
            digital_problems[dp].kmaps[k]={groups:[]};
        }
        var keys=[];
        jQuery( ".selected", jQuery('#kmap-'+dp+'-'+k) ).each(function(i,v) {
            keys.push(jQuery(v).attr('id'));
            jQuery(v).addClass('grouped');
            jQuery(v).removeClass('selected');
        });
        if(keys.length>0){
            digital_problems[dp].kmaps[k].groups.push(keys);
            var gno=digital_problems[dp].kmaps[k].groups.indexOf(keys);
            var gid=kmap_patern_var%10;
            kmap_patern_var++;
            var kmapGroup='kmapgroup'+gid;
            digital_problems[dp].kmaps[k].groups[gno]["groupPattern"]=kmapGroup;
            var duplicateMap=jQuery('#kmap-'+dp+'-'+k).clone().attr('id','kmap-'+dp+'-'+k+'-ig-'+gno);
            var dmdiv=document.createElement('div');
            $(dmdiv).css('display','inline-block');
            $(dmdiv).css('padding','10px');
            $(dmdiv).attr('id','div-kmap-'+dp+'-'+k+'-ig-'+gno);
            $(dmdiv).append(duplicateMap);
            jQuery('#kmap-'+dp+'-'+k+'-IndividualGroups').append($(dmdiv));
            jQuery('.grouped',duplicateMap).each(function(i,v){
                jQuery(v).removeClass().addClass('kmapValues')
            });
             
            for(var i in keys){
                jQuery("#"+keys[i]).addClass(kmapGroup);
                jQuery("#"+keys[i],duplicateMap).addClass(kmapGroup);
            }
        }
        writekmapgroup(k,dp)
        console.log(digital_problems)
    }
}
