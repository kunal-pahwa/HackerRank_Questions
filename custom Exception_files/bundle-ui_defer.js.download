function amphiflip(elementid){
    var fele = $('#'+elementid+' .amphiflipfrontcard'),bele = $('#'+elementid+' .amphiflipbackcard');
    fele.animate({  rotateDegree: 90 }, {
        step: function(now,fx) {
          $(this).css('-webkit-transform','rotateY('+now+'deg)'); 
          $(this).css('-moz-transform','rotateY('+now+'deg)');
          $(this).css('transform','rotateY('+now+'deg)');
        },
        duration:250,
        complete:function(){
            fele.hide();
            fele.removeClass('amphiflipfrontcard');
            fele.addClass('amphiflipbackcard');
            bele.css({
                '-webkit-transform': 'rotateY(-90deg)',
                '-moz-transform': 'rotateY(-90deg)',
                'transform': 'rotateY(-90deg)'
            });
            bele.show();
            bele.animate({rotateDegree: 0}, {
                step: function(now, fx) {
                    $(this).css('-webkit-transform', 'rotateY(' + now + 'deg)');
                    $(this).css('-moz-transform', 'rotateY(' + now + 'deg)');
                    $(this).css('transform', 'rotateY(' + now + 'deg)');
                },
                duration: 250,
                complete: function() {
                    bele.removeClass('amphiflipbackcard');
                    bele.addClass('amphiflipfrontcard');
                }
            }, 'linear');
        }
    },'linear');
}

