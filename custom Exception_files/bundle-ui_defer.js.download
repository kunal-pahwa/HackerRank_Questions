function move_first_to_second(first,second) {
    var len = second.length;
    newOption = document.createElement('option');
    newOption.text = first[first.selectedIndex].text;
    newOption.value = first[first.selectedIndex].value;
    second.options[len] = newOption;
    remove_elem(first.selectedIndex,first);
}

function remove_elem(idx,combo) {
  var len = combo.length;
  if (len <= 0) return;
  var j=0;

  for (var i = 0; i < combo.options.length; i++) {
    if (i == idx) {
      combo.options[i].selected = false;
      combo.options[j].text = combo.options[i+1].text;
      combo.options[j].value = combo.options[i+1].value;
	j++;i++;
    } else {
      combo.options[j].text = combo.options[i].text;
      combo.options[j].value = combo.options[i].value;
      j++;
    }
  }
}

