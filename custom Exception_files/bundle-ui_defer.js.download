        if(data.redirect == 'true'){
            hideWait();
            if(data.enable == 'true' || data.enable == 'null'){
                amphiConfirmDialog("You must attend a session before proceeding",function(){
                    window.location.href=contextPath+"/lessonComponent/redirectToSession/"+data.checkPoint;
                },function(){});
