    alert("langId:"+langId)*/
    params = "groupId="+encodeURIComponent(group)+"&name="+encodeURIComponent(name)+"&version="+encodeURIComponent(version)+"&language.id="+encodeURIComponent(langId)

    $.ajax({
        url: action,
