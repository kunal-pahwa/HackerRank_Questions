function userOnlineFn(url,logOutUrl,time){
    setTimeout(function() {
        $.ajax({
            url:url,
            data: {                
               "dummy":new Date().getTime()
            },
            success:function(data, textStatus) {
                if(data=="1"){
                    $("#amphiHeaderUserIcon").removeClass("amphi-user-offline");
                    $("#amphiHeaderUserIcon").addClass("amphi-user-online");
                }
                else{                       
                    $("#amphiHeaderUserIcon").removeClass("amphi-user-online");
                    $("#amphiHeaderUserIcon").addClass("amphi-user-offline");   
                    if(showOfflineNotification){
                        $.pnotify({
                            delay:60000,
                            animation:"fade",
                            type:"error",        
                            text: "You are offline"
                        });
                    }  
                    if(data == "logout"){
                        $.ajax({url:logOutUrl}); 
                    }
                }
                userOnlineFn(url,logOutUrl,time);
            },
            error:function(data, textStatus) {
                $("#amphiHeaderUserIcon").removeClass("amphi-user-online");
                $("#amphiHeaderUserIcon").addClass("amphi-user-offline"); 
                if(showOfflineNotification){
                    $.pnotify({
                        delay:2000,
                        animation:"fade",
                        type:"failure",        
                        text: "You are offline"
                    });  
                }
                userOnlineFn(url,logOutUrl,time);             
            }
        });  
    }, time);
}

