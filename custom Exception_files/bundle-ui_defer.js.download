function prevResource(ele)
{
    ele=(ele.parentElement).parentElement;
    var i = indexOfActiveChild(ele);
    ele.children[i].style.display='none';
    if(i==ele.children.length-1)
        ele.children[0].children[1].disabled=false;
    i--;
    if(i==1)
        ele.children[0].children[0].disabled=true;
    ele.children[i].style.display='block';
}
function nextResource(ele)
{
    ele=(ele.parentElement).parentElement;
    var i = indexOfActiveChild(ele);
    ele.children[i].style.display='none';
    if(i==1)
        ele.children[0].children[0].disabled=false;
    i++;
    if(i==ele.children.length-1)
        ele.children[0].children[1].disabled=true;
    ele.children[i].style.display='block';
}
function indexOfActiveChild(parent)
{
    $.each($(".dijitAccordionContainer-dijitContentPane"),function(index,val) {
        $(val).css("height","auto")
    });
    for(var i in parent.children)
    {
        if(parent.children[i].style.display=='block')
            return i;
    }
    
    return -1;
}

