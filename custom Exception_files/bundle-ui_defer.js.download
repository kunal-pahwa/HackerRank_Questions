function checkExactValues(id) {
    var allCheckElements = document.getElementsByName(id);
    for(var i=0;i<allCheckElements.length;i++) {
        if(allCheckElements[i].checked) {
            var exacttimeele = document.getElementById('exactTime-'+i);
            var minsbeforeele = document.getElementById('minsBefore-'+i);
            if(allCheckElements[i].checked) {
                exacttimeele.style.display='block';
                minsbeforeele.style.display='none';
            }
            else {
                exacttimeele.style.display='none';
                minsbeforeele.style.display='block';
            }
        }
    }
}

