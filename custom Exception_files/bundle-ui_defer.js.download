    updateVideoCheckPoint: function(checkPoint){
        showWait();
        var checkPointPosition =  document.getElementById('checkPosition').value.trim();
        var checkName =  document.getElementById('checkName').value.trim();
        var checkThreshold =  document.getElementById('checkThreshold').value.trim();
        var checkSessionId =  document.getElementById('checkAmphiSession').value.trim();
        var componentId =  document.getElementById('componentId')!=undefined ? document.getElementById('componentId').value.trim() : '';
        var pgNoFrom =  document.getElementById('pgNoFrom')!=undefined ? document.getElementById('pgNoFrom').value.trim() : '';
        var pgNoTo =  document.getElementById('pgNoTo')!=undefined ? document.getElementById('pgNoTo').value.trim() : '';
        if(checkPointPosition == '' || checkPointPosition == null || checkName == '' || checkName == null){
            document.getElementById('createCheckPointError').innerHTML = '';
            document.getElementById('createCheckPointError').innerHTML = '*Missed out Some Fields';
            hideWait();
            return;
        }
        $.ajax({
            type: "POST",
            url: contextPath+"/lessonComponent/updateVideoCheckPoint",
            data: { 
                'checkPointId': checkPoint,
                'checkPointPosition': checkPointPosition,
                'checkName': checkName,
                'checkThreshold': checkThreshold,
                'checkSessionId': checkSessionId,
                'componentId':componentId,
                'pgNoFrom':pgNoFrom,
                'pgNoTo':pgNoTo
            },
            dataType: "json",
            success: function(data) {
                if(data.position == 'true'){
                    alert('Unable to Save! Already CheckPoint has been created at this Position');
                    hideWait();
                    return;
                }
                if(data.mapSession == 'true'){
                    alert('Unable to Save! Session has been mapped to one of the Checkpoints created earlier');
                    hideWait();
                    return;
                }
                lessonComponent.fetchVideoCheckPointList(checkPoint, 'update')
            },
            error: function (request, status, error) {
                hideWait();
                alert(error);
            }
          });
    },
    
    removeVideoCheckPoint: function(checkPoint){
        showWait();
        if(!confirm('Are you sure that you want to permanently delete the selected Check Point?')){
            hideWait();
            return;
        }
        $.ajax({
            type: "POST",
            url: contextPath+"/lessonComponent/removeVideoCheckPoint",
            data: { 
                'id': checkPoint
            },
            dataType: "json",
            success: function(data) {
                //alert(data);
                lessonComponent.fetchVideoCheckPointList(data.id, 'save')
            },
            error: function (request, status, error) {
                hideWait();
                alert(error);
            }
          });
    },
    finalizeVideoCheckPoint: function(lessCompId){
        showWait();
        if(!confirm('Are you sure No more checkPoints Manipuation takes place?')){
            hideWait();
            return;
        }
        $.ajax({
            type: "POST",
            url: contextPath+"/lessonComponent/finalizeVideoCheckPoint",
            data: { 
                'id': lessCompId
            },
            dataType: "json",
            success: function(data) {
                if(data.flag == 'true'){
                    alert('Now you can Attempt the Video Component');
                }
                else{
                    alert('Unable to process your request, kindly contact Administrator');
                }
                hideWait();
            },
            error: function (request, status, error) {
                hideWait();
                alert(error);
            }
          });
    },
