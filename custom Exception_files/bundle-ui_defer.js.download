var preDocumentContentLength
function measureContentMetrics(probId) {
    var contentStr = "";
    var length = 0;
    collectCKeditorValues();
    contentStr = $("#editTopiccontent_"+probId).val();
//    if ($("#editTopiccontent").hasClass("richtexteditor")) {
//        var editor = CKEDITOR.instances.editTopiccontent;
//        contentStr = editor.getData()
//    }    
//    else{
//        contentStr = $("#editTopiccontent").val();
//    }
    length = countCharacters(contentStr);
    contentStr = contentStr.replace(/<[^<|>]+?>/gi,"");
    if(contentStr.length != preDocumentContentLength) {
        var metrics = document.getElementById("contentMetrics").value
        metrics = metrics + new Date().getTime() + ";"+length+ ";" + countWords(contentStr) + ","
        document.getElementById("contentMetrics").value = metrics
        preDocumentContentLength = contentStr.length
}
}

function countCharacters(str){
    var unSpacedString = str.trim();
    return unSpacedString.length;
}
function countWords(str){
    var y = str;
    var r = 0;
    a=y.replace(/\s/g,' ');
    a=a.split(' ');
    for (z=0; z<a.length; z++) {if (a[z].length > 0) r++;}
    return r;
} 
