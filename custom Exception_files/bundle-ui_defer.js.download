    function clipResource(pUrl,compId){
        if($("#slideId").val().length > 0){
            showWait();
            var stTime = $("#clipStart").html();
            var edTime = $("#clipEnd").html();            
            var diff = edTime-stTime;
            var time;                
            var hours = Math.floor(diff / 3600);
            if(hours < 10)
                hours = "0"+hours;
            diff %= 3600;
            var minutes = Math.floor(diff / 60);
            if(minutes < 10)
                minutes = "0"+minutes;
            var seconds = parseInt(diff % 60);
            if(seconds < 10)
                seconds = "0"+seconds;
            time = hours+":"+minutes+":"+seconds;
            $.ajax({
                type:"POST",
                url:pUrl,
                data:{
                    clipFrom : stTime,
                    clipTo : time,
                    url : $("#selectedVideo").attr("href"),
                    id : compId,
                    slideId : $("#slideId").val()
                },
                success : function(data){                    
                    if(data!="Error"){
                        $("#clipedResource").attr("href",data);
                        $("#resourcePath").val(data);
                        $("#clipedResource").parent().css("display","inline");
                        loadFlowPlayer("clipedPlayer"); 
                    }else{
                        alert("Please select any one slide then Clip");
                    }
                    hideWait();
