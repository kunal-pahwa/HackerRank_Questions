function countKeywordsFromCode(code) {
    var count = 0,i=0;
    for(i=0;i<keyList.length;i++) {
        var patt = new RegExp("\\s"+keyList[i],'g');
        var x = code.match(patt);
        if(x!= null) {
            count = count+x.length;
        }

    }
    return count;
}
