function writekmapgroup(km,dp){
    var id="#group-p-"+dp+"-kmap-"+km
    var st=''
    for(var i in digital_problems[dp].kmaps[km].groups){
        if(digital_problems[dp].kmaps[km].groups[i]!=undefined){
            if(st!=''){
                st+='|'
            }
            var g=''
            for(var j in digital_problems[dp].kmaps[km].groups[i]){
                if(j!="groupPattern"){
                    var key=digital_problems[dp].kmaps[km].groups[i][j]
                    key=key.replace(("kmv-"+dp+"-"+km+"-"),"")
                    if(g!=''){
                        g+=','
                    }
                    g+=key
                }
            }
            st+=g
        }
    }
    jQuery(id).val(st);
}
