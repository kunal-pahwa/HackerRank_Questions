    checkFileFormat: function(fileName){
        
        if(fileName.lastIndexOf('.')){
            var format;
            if(fileName.substring(fileName.lastIndexOf('.')+1).length>0){
                format = fileName.substring(fileName.lastIndexOf('.')+1)
                if(format == 'pdf'||format == 'webm'||format == 'mp4'||format == 'flv' || format == 'ogg'){
                    if(format == 'pdf')
                        document.getElementById('typePDF').checked = true;
                    else
                        document.getElementById('typeVideo').checked = true;
                    return true;
                }
            }
        }
        return false;
    }
}
