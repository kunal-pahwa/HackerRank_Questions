function getCopiedContent(text1,text2) {
    var dmp = new diff_match_patch();
    dmp.Diff_Timeout = 0;
    var d = dmp.diff_main(text1, text2, false);
    var copiedTxt = ""
    for(i=0;i<d.length;i++) {
	  var obj = d[i]
          if(obj[0] == 1 || obj[0] == -1)
            copiedText = obj[1]
    }
    return copiedText
}
