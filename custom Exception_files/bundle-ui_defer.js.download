function reportTemplateDraggableInit(){
    var onDrop = function($item, targetContainer, _super) {
        $item.removeClass('cantDrop');
        $item.removeClass('canDrop');
        var clonedItem = $('<li/>').css({
            height: 0
        });
        if (targetContainer.options.drop && targetContainer.options.drag) {
            $item.before(clonedItem);
            clonedItem.animate({
                'height': $item.height()
            });
            $item.animate(clonedItem.position(), function() {
                clonedItem.detach();
                _super($item);
            });
            if($item.attr('amphiExistingElement') == null){
                $item.dblclick(editReportTemplateEleParams);
                $item.dblclick();
                $item.attr('amphiExistingElement',true);
                $item.prepend($("<a href='javascript:void(0)' onclick='this.parentNode.parentNode.removeChild(this.parentNode)' class='label label-danger' style='float:right' ><span class='glyphicon glyphicon-remove' ></span></a>"));
            }
        }
        else{
            $item.remove();
        }
        resizeCenterPane();
    };
    
    var onDragStart = function($item, container, _super) {
        var offset = $item.offset(),
                pointer = container.rootGroup.pointer;
        dragAndDropAdjustment = {
            left: pointer.left - offset.left,
            top: pointer.top - offset.top
        };
        //console.log(container)
        if(!container.options.drop)
            $item.clone().insertAfter($item);
        _super($item, container);
    };
    var onDrag = function($item, position) {
        if ($(document).find("li.placeholder").length > 0) {
            $item.removeClass('cantDrop');
            $item.addClass('canDrop');
        }
        else {
            $item.removeClass('canDrop');
            $item.addClass('cantDrop');
        }
        $item.css({
            left: position.left - (sortableMobile ? 0 : dragAndDropAdjustment.left),
            top: position.top - (sortableMobile ? 0 : dragAndDropAdjustment.top)
        });
    };
    $("ul.reportTemplateDraggableDragOnly").sortable({
        group: "reportTemplate",
        pullPlaceholder: false,
        onDrop: onDrop,
        onDragStart: onDragStart,
        onDrag: onDrag,
        drop:false
    });
    $("ul.reportTemplateDraggable").sortable({
        group: "reportTemplate",
        pullPlaceholder: false,
        onDrop: onDrop,
        onDragStart: onDragStart,
        onDrag: onDrag,
        afterMove: function (placeholder, container) {
          if(oldContainer != container){
            oldContainer = container
          }
        }
    });
    $("ul.reportTemlpateDraggableJunk").sortable({
        group: "reportTemplate",
        pullPlaceholder: false,
        drag:false
    });
}
